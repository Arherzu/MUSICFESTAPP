@model IEnumerable<musicfest.Models.Contacto>

<h2>Lista de Solicitudes</h2>

<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
                
                
                <th scope="col">
                    Nombre
                </th>
                <th scope="col">
                    ApellidoPaterno
                </th>
                <th scope="col">
                    ApellidoMaterno
                </th>
                <th scope="col">
                    Celular
                </th>
                <th scope="col">
                    Direcci√≥n
                </th>
                <th scope="col">
                    Distrito
                </th>
                <th scope="col">
                    Nacionalidad
                </th>
                <th scope="col">
                    Mensaje
                </th>
        </tr>
    </thead>
    <tbody>

@foreach (var item in Model)
{
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.apellido_paterno )
            </td>
             
            <td>
                @Html.DisplayFor(modelItem => item.apellido_materno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.celular)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.direccion)
            </td>     
            <td>
                @Html.DisplayFor(modelItem => item.distrito)
            </td>  
            <td>
                @Html.DisplayFor(modelItem => item.nacionalidad)
            </td>  
            <td>
                @Html.DisplayFor(modelItem => item.Mensaje)
            </td>                                
        </tr>
}        
    </tbody>
</table>